<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LevelsActivity">

    <EditText
        android:id="@+id/enterNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.200"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055" />

    <TextView
        android:id="@+id/firstLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enterNumber"
        app:layout_constraintVertical_bias="0.018" />

    <TextView
        android:id="@+id/secondLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstLevel"
        app:layout_constraintVertical_bias="0.028" />

    <TextView
        android:id="@+id/thirdLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secondLevel"
        app:layout_constraintVertical_bias="0.033" />

    <TextView
        android:id="@+id/fourthLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/thirdLevel"
        app:layout_constraintVertical_bias="0.029" />

    <TextView
        android:id="@+id/fifthLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fourthLevel"
        app:layout_constraintVertical_bias="0.033" />

    <TextView
        android:id="@+id/sixthLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.397"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fifthLevel"
        app:layout_constraintVertical_bias="0.038" />


    <TextView
        android:id="@+id/seventhLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.392"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sixthLevel"
        app:layout_constraintVertical_bias="0.052" />

    <TextView
        android:id="@+id/eighthLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
