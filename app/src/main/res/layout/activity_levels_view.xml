        android:textSize="19sp"
        android:fontFamily="sans-serif-black"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.400"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/seventhLevel"
        app:layout_constraintVertical_bias="0.250" />

    <TextView
        android:id="@+id/ninethLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="19sp"
        android:fontFamily="sans-serif-black"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.400"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/eighthLevel"
        app:layout_constraintVertical_bias="0.250" />

    <TextView
        android:id="@+id/tenthLevel"
        android:layout_width="630dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="text"
        android:textSize="19sp"
        android:fontFamily="sans-serif-black"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.400"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ninethLevel"
        app:layout_constraintVertical_bias="0.250" />

    <Button
        android:id="@+id/addNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add_number_to_set"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.942"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.051" />

    <Button
        android:id="@+id/seeLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/see_level"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.942"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/addNumber"
        app:layout_constraintVertical_bias="0.051" />

</androidx.constraintlayout.widget.ConstraintLayout>